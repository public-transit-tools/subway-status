{
  "version": 2,
  "installCommand": "npm install",
  "builds": [
    { "src": "cities/NYC/next.config.mjs", "use": "@vercel/next" },
    { "src": "cities/TTC/next.config.mjs", "use": "@vercel/next" },
    { "src": "Landing Page/next.config.mjs", "use": "@vercel/next" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "cities/NYC/$1", "has": [{ "type": "host", "value": "nyc.subwaystatus.com" }] },
    { "src": "/(.*)", "dest": "cities/TTC/$1", "has": [{ "type": "host", "value": "ttc.subwaystatus.com" }] },
    { "src": "/(.*)", "dest": "Landing Page/$1", "has": [{ "type": "host", "value": "subwaystatus.com" }] },
    { "src": "/(.*)", "dest": "Landing Page/$1", "has": [{ "type": "host", "value": "www.subwaystatus.com" }] }
  ]
}